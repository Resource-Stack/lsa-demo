<p id="notice"><%= notice %></p>

                        <div class="col-xl-12 col-lg-12">
                            <div class="card shadow mb-4">
                                <!-- Card Header - Dropdown -->
                                <div
                                    class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
                                    <h6 class="m-0 font-weight-bold text-primary">Data Dump Dictionaries</h6>
                                    <div class="dropdown no-arrow">
                                        <a class="dropdown-toggle" href="#" role="button" id="dropdownMenuLink"
                                            data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                            <i class="fa fa-ellipsis-v fa-sm fa-fw text-gray-400"></i>
                                        </a>
                                        <div class="dropdown-menu dropdown-menu-right shadow animated--fade-in"
                                            aria-labelledby="dropdownMenuLink">
                                            <div class="dropdown-header">Action:</div>
                                              <%= link_to 'New Data Dump Dictionary', new_data_dump_dictionary_path %>  
                                        </div>
                                    </div>
                                </div>
                                <!-- Card Body -->
                                <div class="card-body">
                                  <table class="table table-striped">
                                    <thead>
                                      <tr>
                                        <th>User</th>
                                        <th>Source</th>
                                        <th>Csv header name</th>
                                        <th colspan="3"></th>
                                      </tr>
                                    </thead>

                                    <tbody>
                                      <% @data_dump_dictionaries.each do |data_dump_dictionary| %>
                                        <tr>
                                          <td><%= data_dump_dictionary.user_id %></td>
                                          <td><%= data_dump_dictionary.source_id %></td>
                                          <td><%= data_dump_dictionary.csv_header_name %></td>
                                          <td><%= link_to 'Show', data_dump_dictionary %></td>
                                          <td><%= link_to 'Edit', edit_data_dump_dictionary_path(data_dump_dictionary) %></td>
                                          <td><%= link_to 'Destroy', data_dump_dictionary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                                        </tr>
                                      <% end %>
                                    </tbody>
                                  </table>

                                  <br>

                                </div>
                            </div>
                        </div>






  
