<!DOCTYPE html>
<html>
  <head>
    <title>LsaRuby</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
  </head>

  <body>

    <nav class="nav">
      <%= link_to 'Home', root_path, class: 'nav-link' %>
      <%= link_to 'Master Table', master_tables_path, class: 'nav-link' %>
      <%= link_to 'Table Entries', table_entries_path, class: 'nav-link' %>
      <%= link_to 'Data Dictionary', data_dictionaries_path, class: 'nav-link' %>
      <%= link_to 'data_dump_dictionary', data_dump_dictionaries_path, class: 'nav-link' %>
      <%= link_to 'CSV Uploads', csv_uploads_path, class: 'nav-link' %>
    </nav>
    <%= yield %>
  </body>
</html>




