    <div class="col-xl-12 col-lg-12">
        <div class="card shadow mb-4">
            <!-- Card Header - Dropdown -->
            <div
                class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
                <h6 class="m-0 font-weight-bold text-primary">My Uploads</h6>
                  <ul class="nav justify-content-end">
                    <li class="nav-item">

                      <%= link_to 'New Upload', new_csv_upload_path, class: 'dropdown-item' %>

                    </li>
                  </ul>
            </div>
            <!-- Card Body -->
            <div class="card-body">
                <table class="table table-striped">
                  <thead>
                    <tr>

                        <th> CSV Filename </th>
                        <th> Upload Date </th>
                        <th> Index </th>
                        <th colspan="3"></th>

                    </tr>
                  </thead>

                  <tbody>
                    <% @csv_uploads.each do |csv_upload| %>
                    <tr>
                        <td> <%= csv_upload.csv_file.filename %> </td>
                        <td> <%= csv_upload.csv_upload_date %> </td>
                        <td> <%= csv_upload.logstash_index %> </td>
                        <td><%= link_to 'Destroy', csv_upload, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                    </tr> 
                    <% end %>
                  </tbody>
              </table>
          </div>
      </div>
  </div>
