<!-- Page Heading -->
<div class="d-sm-flex align-items-center justify-content-between mb-4">
    <h1 class="h3 mb-0 text-gray-800">Find By Value Report</h1>
    <a href="#" class="d-none d-sm-inline-block btn btn-sm btn-primary shadow-sm"><i
            class="fa fa-download fa-sm text-white-50"></i> Generate Report</a>
</div>

<table class="table table-stripped">
  <thead>
    <tr>
      <% @masterRow.each do |x| %>
        <th> <%= x %></th>
      <% end %>

    </tr>
  </thead>

  <tbody>
  <% @FindByTableEntries.each do |x| %>
      <tr>
        <td><%= x.field_one %></td>
        <td><%= x.field_two %></td>
        <td><%= x.field_three %></td>
        <td><%= x.field_four %></td>
        <td><%= x.field_five %></td>
        <td><%= x.field_six %></td>
        <td><%= x.field_seven %></td>
        <td><%= x.field_eight %></td>
        <td><%= x.field_nine %></td>
        <td><%= x.field_ten %></td>
      </tr> 
    <% end %>
  </tbody>
</table>
