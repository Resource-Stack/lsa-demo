<p id="notice"><%= notice %></p>

<h1>Master Tables</h1>

<table class="table table-stripped">
  <thead>
    <tr>
      <th>Field one</th>
      <th>Field two</th>
      <th>Field three</th>
      <th>Field 4</th>
      <th>Field 5</th>
      <th>Field 6</th>
      <th>Field 7</th>
      <th>Field 8</th>
      <th>Field 9</th>
      <th>Field 10</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @master_tables.each do |master_table| %>
      <tr>
        <td><%= master_table.field_one %></td>
        <td><%= master_table.field_two %></td>
        <td><%= master_table.field_three %></td>
        <td><%= master_table.field_four %></td>
        <td><%= master_table.field_five %></td>
        <td><%= master_table.field_six %></td>
        <td><%= master_table.field_seven %></td>
        <td><%= master_table.field_eight %></td>
        <td><%= master_table.field_nine %></td>
        <td><%= master_table.field_ten %></td>
        <td><%= link_to 'Show', master_table %></td>
        <td><%= link_to 'Edit', edit_master_table_path(master_table) %></td>
        <td><%= link_to 'Destroy', master_table, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Master Table', new_master_table_path %>
