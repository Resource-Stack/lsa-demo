<p id="notice"><%= notice %></p>

<h1>Data Dump Dictionaries</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Source</th>
      <th>Csv header name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @data_dump_dictionaries.each do |data_dump_dictionary| %>
      <tr>
        <td><%= data_dump_dictionary.user_id %></td>
        <td><%= data_dump_dictionary.source_id %></td>
        <td><%= data_dump_dictionary.csv_header_name %></td>
        <td><%= link_to 'Show', data_dump_dictionary %></td>
        <td><%= link_to 'Edit', edit_data_dump_dictionary_path(data_dump_dictionary) %></td>
        <td><%= link_to 'Destroy', data_dump_dictionary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Data Dump Dictionary', new_data_dump_dictionary_path %>
