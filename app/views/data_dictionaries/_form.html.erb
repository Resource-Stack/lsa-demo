      <h2><%= pluralize(data_dictionary.errors.count, "error") %> prohibited this data_dictionary from being saved:</h2>

      <ul>
      <% data_dictionary.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <p>For testing, use user 333</p> 
  <div class="field">
    <%= form.label :user_id %>
